@using NBG.Visitor.Storage.Dtos;

@switch (subpage)
{
    case 0:
        <TerminalHome OnNext=@(() => subpage = 1) />
        break;
    case 1:
        <TerminalRegister FormData=form OnBack=@(() => subpage = 0) OnNext=@(() => Console.WriteLine(form)) />
        break;
}

@code {
    private int subpage = 0;

    private RegisterFormData form = new();
}
